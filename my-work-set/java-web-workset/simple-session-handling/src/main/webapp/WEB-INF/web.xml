<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>loginActionServlet</servlet-name>
		<servlet-class>com.ck.servlet.LoginActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginActionServlet</servlet-name>
		<url-pattern>/loginActionServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>nextActionServlet</servlet-name>
		<servlet-class>com.ck.servlet.NextActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>nextActionServlet</servlet-name>
		<url-pattern>/nextActionServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logoutActionServlet</servlet-name>
		<servlet-class>com.ck.servlet.LogoutActionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logoutActionServlet</servlet-name>
		<url-pattern>/logoutActionServlet</url-pattern>
	</servlet-mapping>


	<login-config>
		<auth-method>BASIC</auth-method>
	</login-config>

</web-app>
